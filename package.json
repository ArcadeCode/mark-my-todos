{
    "name": "mark-my-todos",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "frontend": "vite --config vite.config.ts",
        "backend": "bun ./backend/server.ts",
        "dev": "npm-run-all2 --parallel frontend backend",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "@types/bun": "^1.2.19",
        "hono": "4.8.9",
        "kleur": "^4.1.5",
        "vue": "^3.5.17",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^22.15.32",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/tsconfig": "^0.7.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.5.3",
        "typescript": "~5.8.0",
        "vite": "^7.0.0",
        "vite-plugin-vue-devtools": "^7.7.7",
        "vue-tsc": "^2.2.10"
    }
}
